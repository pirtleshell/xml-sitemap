{
  "name": "xml-sitemap",
  "version": "0.2.1",
  "description": "Utilities for quickly writing XML sitemaps",
  "main": "lib/index.js",
  "scripts": {
    "docs": "./node_modules/jsdoc-to-markdown/bin.js -t jsdoc2md/api.hbs lib/*.js > docs/api.md",
    "test": "xo && mocha test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PirtleShell/xml-sitemap.git"
  },
  "keywords": [
    "xml",
    "sitemap",
    "sitemaps",
    "sitemap.xml",
    "generate",
    "utility",
    "website",
    "write-sitemaps",
    "edit-sitemaps"
  ],
  "author": "Robert Pirtle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PirtleShell/xml-sitemap/issues"
  },
  "homepage": "https://github.com/PirtleShell/xml-sitemap#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "url": "^0.11.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "jsdoc-to-markdown": "^2.0.1",
    "mocha": "^3.2.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "overrides": [
      {
        "files": "test/**",
        "env": "mocha",
        "rules": {
          "import/no-unassigned-import": 0,
          "no-unused-expressions": 0,
          "padded-blocks": 0,
          "max-nested-callbacks": 0
        }
      }
    ]
  }
}
